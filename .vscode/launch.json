  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Main",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "node",
      "runtimeArgs": [
        "--inspect",
        "--loader",
        "tsx",
        "main.ts",
        "s3",
        "--help"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
  ]
}
